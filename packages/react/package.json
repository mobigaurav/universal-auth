{
  "name": "@universal-auth/react",
  "version": "1.0.4",
  "description": "React components and hooks for universal auth module",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
